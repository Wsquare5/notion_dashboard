<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Service label (must be unique) -->
    <key>Label</key>
    <string>com.binance.daily_update</string>
    
    <!-- Program to run -->
    <key>ProgramArguments</key>
    <array>
        <string>/Users/wanjinwoo/Desktop/Work/trading/Binance/scripts/daily_update.sh</string>
    </array>
    
    <!-- Working directory -->
    <key>WorkingDirectory</key>
    <string>/Users/wanjinwoo/Desktop/Work/trading/Binance</string>
    
    <!-- Schedule: Run every day at 6:30 AM -->
    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>6</integer>
        <key>Minute</key>
        <integer>30</integer>
    </dict>
    
    <!-- Standard output and error logs -->
    <key>StandardOutPath</key>
    <string>/Users/wanjinwoo/Desktop/Work/trading/Binance/logs/launchd_stdout.log</string>
    
    <key>StandardErrorPath</key>
    <string>/Users/wanjinwoo/Desktop/Work/trading/Binance/logs/launchd_stderr.log</string>
    
    <!-- Run even when user is not logged in (optional) -->
    <key>RunAtLoad</key>
    <false/>
    
    <!-- Environment variables (if needed for Python) -->
    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/opt/homebrew/bin</string>
    </dict>
</dict>
</plist>
